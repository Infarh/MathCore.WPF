<Application x:Class="MathCore.WPF.WindowTest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:l="clr-namespace:MathCore.WPF.WindowTest"
             StartupUri="TestWindow1.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="pack://application:,,,/MathCore.WPF;component/Styles/ModernWindowStyle.xaml"/>


                <ResourceDictionary Source="Resources/Themes/Main.xaml"/>
                <ResourceDictionary Source="Resources/AppCommands.xaml"/>

            </ResourceDictionary.MergedDictionaries>

            <l:ServiceLocator x:Key="Locator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
